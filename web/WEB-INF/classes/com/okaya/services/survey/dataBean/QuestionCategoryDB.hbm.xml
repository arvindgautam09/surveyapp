<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.okaya.services.survey.dataBean.QuestionCategoryDB" table="question_category">
		
		<id name="qsc_id" column="qsc_id">
		<generator class="increment"/>
		</id>
		<property name="qsc_name"  type="java.lang.String" column="qsc_name"/>
				
	</class>
</hibernate-mapping>
