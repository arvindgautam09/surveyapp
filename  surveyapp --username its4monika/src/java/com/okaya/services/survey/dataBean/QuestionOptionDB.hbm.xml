<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.okaya.services.survey.dataBean.QuestionOptionDB"
		table="question_option">

		<id name="qso_id" column="qso_id">
			<generator class="increment" />
		</id>

		<property name="qso_qsm_id" column="qso_qsm_id" />
		<property name="qso_option" type="java.lang.String"
			column="qso_option" />
		<property name="qso_type" column="qso_type" />
		

	</class>
</hibernate-mapping>
