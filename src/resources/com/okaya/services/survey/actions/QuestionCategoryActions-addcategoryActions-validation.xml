<!DOCTYPE validators PUBLIC 
  		"-//OpenSymphony Group//XWork Validator 1.0.2//EN"  
  		"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="questionCategory">
	<field-validator type="requiredstring">
			<message key="requiredstring" >Category name is required</message>
		</field-validator>
	</field>
	<field name="questionCategoryDelete">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Category name is required</message>
		</field-validator>
	</field>
	<field name="editquestionCategory">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Category name is required</message>
		</field-validator>
	</field>
	<field name="questionCategNew">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Category name is required</message>
		</field-validator>
	</field>
</validators>


